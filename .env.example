# GPU Configuration
CUDA_VISIBLE_DEVICES=0

# Gemini API Key for prompt generation
# Get your key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# PostgreSQL Database
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5433/dbname
POSTGRES_PASSWORD=your_postgres_password

# JWT Secret Key (generate a secure random string for production)
SECRET_KEY=your_secret_key_here

# Celery Configuration (for parallel GPU processing)
# Set to true only if you have multiple GPUs and Redis running
USE_CELERY=false
REDIS_URL=redis://localhost:6379/0
